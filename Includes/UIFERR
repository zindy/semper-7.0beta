c
      integer NOTNUM
      parameter (NOTNUM=45)
c
c     "Unable to number to a string"
c
      integer NOTCON
      parameter (NOTCON=46)
c
c     "Invalid menu identifier"
c
      integer BADMEN
      parameter (BADMEN=47)
c
c     "Invalid menu type"
c
      integer BADMTY
      parameter (BADMTY=48)
c
c     "Invalid menu style"
c
      integer BADMST
      parameter (BADMST=49)
c
c     "Menu panel has not been created"
c
      integer MPNOTC
      parameter (MPNOTC=50)
c
c     "Action string is too long to fit application input buffer"
c
      integer ACLONG
      parameter (ACLONG=51)
c
c     "Framestore access error"
c
      integer FSERR
      parameter (FSERR=52)
c
c     "UIF is not running"
c
      integer NOTRUN
      parameter (NOTRUN=53)
c
c     "Window of requested size/position will not fit device"
c
      integer NOTFIT
      parameter (NOTFIT=54)
c
c     "UIF termination failed"
c
      integer BADTER
      parameter (BADTER=55)
c
c     "Cursor position stack is empty"
c
      integer STKEMP
      parameter (STKEMP=56)
c
c     "Cursor position stack is full"
c
      integer STKFUL
      parameter (STKFUL=57)
c
