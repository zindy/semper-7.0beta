ac_f77_v_output=`eval $ac_link 5>&1 2>&1 |
  sed '/^Driving:/d; /^Configured with:/d;
      '"/^[_$as_cr_Letters][_$as_cr_alnum]*=/d"`
$as_echo "$ac_f77_v_output" >&5
FFLAGS=$ac_save_FFLAGS

