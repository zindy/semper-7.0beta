C Ascii character set parameters
      INTEGER KSPACE,KPLING,KDQUOT,KHASH,KDOLLA
      INTEGER KPCENT,KAMPER,KQUOTE,KBRA,KKET,KSTAR
      INTEGER KPLUS,KCOMMA,KMINUS,KDOT,KSLASH
      INTEGER KZERO,KONE,KTWO,KTHREE,KFOUR,KFIVE
      INTEGER KSIX,KSEVEN,KEIGHT,KNINE,KCOLON
      INTEGER KSEMIC,KABRA,KEQUAL,KAKET,KQUEST,KAT
      INTEGER KUCA,KUCB,KUCC,KUCD,KUCE,KUCF
      INTEGER KUCG,KUCH,KUCI,KUCJ,KUCK,KUCL
      INTEGER KUCM,KUCN,KUCO,KUCP,KUCQ,KUCR
      INTEGER KUCS,KUCT,KUCU,KUCV,KUCW,KUCX
      INTEGER KUCY,KUCZ,KSBRA,KBACKS,KSKET,KUP
      INTEGER KUNDER,KGRAVE,KLCA,KLCB,KLCC,KLCD
      INTEGER KLCE,KLCF,KLCG,KLCH,KLCI,KLCJ
      INTEGER KLCK,KLCL,KLCM,KLCN,KLCO,KLCP
      INTEGER KLCQ,KLCR,KLCS,KLCT,KLCU,KLCV
      INTEGER KLCW,KLCX,KLCY,KLCZ,KCBRA,KBAR
      INTEGER KCKET,KTILDE
C
      PARAMETER (KSPACE=32,KPLING=33,KDQUOT=34,KHASH=35,KDOLLA=36)
      PARAMETER (KPCENT=37,KAMPER=38,KQUOTE=39,KBRA=40,KKET=41,KSTAR=42)
      PARAMETER (KPLUS=43,KCOMMA=44,KMINUS=45,KDOT=46,KSLASH=47)
      PARAMETER (KZERO=48,KONE=49,KTWO=50,KTHREE=51,KFOUR=52,KFIVE=53)
      PARAMETER (KSIX=54,KSEVEN=55,KEIGHT=56,KNINE=57,KCOLON=58)
      PARAMETER (KSEMIC=59,KABRA=60,KEQUAL=61,KAKET=62,KQUEST=63,KAT=64)
      PARAMETER (KUCA=65,KUCB=66,KUCC=67,KUCD=68,KUCE=69,KUCF=70)
      PARAMETER (KUCG=71,KUCH=72,KUCI=73,KUCJ=74,KUCK=75,KUCL=76)
      PARAMETER (KUCM=77,KUCN=78,KUCO=79,KUCP=80,KUCQ=81,KUCR=82)
      PARAMETER (KUCS=83,KUCT=84,KUCU=85,KUCV=86,KUCW=87,KUCX=88)
      PARAMETER (KUCY=89,KUCZ=90,KSBRA=91,KBACKS=92,KSKET=93,KUP=94)
      PARAMETER (KUNDER=95,KGRAVE=96,KLCA=97,KLCB=98,KLCC=99,KLCD=100)
      PARAMETER (KLCE=101,KLCF=102,KLCG=103,KLCH=104,KLCI=105,KLCJ=106)
      PARAMETER (KLCK=107,KLCL=108,KLCM=109,KLCN=110,KLCO=111,KLCP=112)
      PARAMETER (KLCQ=113,KLCR=114,KLCS=115,KLCT=116,KLCU=117,KLCV=118)
      PARAMETER (KLCW=119,KLCX=120,KLCY=121,KLCZ=122,KCBRA=123,KBAR=124)
      PARAMETER (KCKET=125,KTILDE=126)
C
C Function parameters, control codes etc.
C
C KBRET = return key
C KBTAB = TAB key
C KBESC = ESC key
C KBDEL = Delete character key
C KBKILL = Delete line key
C KBINS = Insert/Overstrike toggle key
C KBHOME,KBEND = Move to beginning/end of line
C KBREFL = Refresh line
C KBUP, KBDOWN, KBLEFT, KBRITE = Up, down, left and right arrow keys
C KBFUNC = Beginning of function key sequence
C KBFMAX = End of function key sequence (max. no. function keys = 256)
C KMBUT = passed back if mouse button close detected during keyboard
C         input via INKEY ONLY.  Passed back as KMBUT + button number
C KMBMAX = End of mouse button sequence (max. no. mouse buttons = 256)
C KBNONE = No key/button press, undefined input for no-wait input
C         --------------------
C
      INTEGER KBRET, KBTAB, KBESC
      INTEGER KBDEL,KBKILL
      INTEGER KBINS,KBHOME,KBEND,KBREFL
      INTEGER KBUP,KBDOWN,KBLEFT,KBRITE
      INTEGER KBFUNC,KBFMAX
      INTEGER KMBUT,KMBMAX
      INTEGER KBNONE
C
      PARAMETER (KBRET=13, KBTAB=9, KBESC=27)
      PARAMETER (KBDEL=257, KBKILL=258)
      PARAMETER (KBINS=259, KBHOME=260, KBEND=261, KBREFL=262)
      PARAMETER (KBUP=513, KBDOWN=514, KBLEFT=515, KBRITE=516)
      PARAMETER (KBFUNC=768, KBFMAX=1023)
      PARAMETER (KMBUT=1024, KMBMAX=1279)
      PARAMETER (KBNONE=-1)
